<h1>Login</h1>
   <%= form_tag sessions_path do %>
  <div class="contentcontainer med left" style="margin-left: 10px;">
   <%= label_tag :Email%>
   <%= text_field_tag :email %>
   <br><%= label_tag :password%>
   <%= password_field_tag :password_enc%>
   <br><%= submit_tag "Login" %>
 </div>
<%end%>