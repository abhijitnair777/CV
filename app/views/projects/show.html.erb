<div class="contentcontainer med left" style="margin-left: 15px;">
<table>
 	<thead>
 		<tr>
 			<th> project_id </th>
 			<th> project_name </th>
 			<th> Description </th>
 			<th> url </th>
 		</tr>
 	</thead>	
 	<tbody>
    <% current_user.projects.all.each do |project| %>
      <tr>
        <td><%= project.id%></td>
        <td><%= project.project_name%></td>
        <td><%= project.description%></td>
        <td><%= project.url%></td>
        <td><%= link_to "Update", edit_project_path(project.id), method: :get, :class => "btn btn-outline-primary" %></td>
        <td><%= link_to "Delete", edit_project_path(project.id), method: :delete, :class => "btn btn-outline-primary" %></td>
      </tr>
    <%end%>
  </tbody>  	
</table>
</div>
