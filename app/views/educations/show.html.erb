<div class="contentcontainer med left" style="margin-left: 15px;">
<table>
  <thead>
    <tr>
      <th> Qualification Type </th>
      <th> Institute Name </th>
      <th> Year of Passing </th>
      <th> Percentage </th>
    </tr>
  </thead>  
  <tbody>
    <% current_user.educations.all.each do |education| %>
      <tr>
        <td><%= education.qualification.qualification_type%></td>
        <td><%= education.institute_name%></td>
        <td><%= education.year_of_passing%></td>
        <td><%= education.percentage%></td>

        <td><%= link_to "Update", edit_education_path(current_user.id), method: :get, :class => "btn btn-outline-primary" %></td>
      </tr>
    <%end%>
  </tbody>
</table>
</div>
